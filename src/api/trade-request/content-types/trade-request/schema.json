{
  "kind": "collectionType",
  "collectionName": "trade_requests",
  "info": {
    "singularName": "trade-request",
    "pluralName": "trade-requests",
    "displayName": "TradeRequest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "accepted",
        "rejected"
      ],
      "default": "pending"
    },
    "createdDt": {
      "type": "datetime",
      "required": true
    },
    "creator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "receiver": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "cardOffered": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::album-card.album-card"
    },
    "cardRequested": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::album-card.album-card"
    }
  }
}
